 #EDIT | My_product

 <%= simple_form_for @my_product, url: my_product_path do |f| %>
    <%= f.error_notification %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :user_id %>
    <%= f.input :category_id %>
    <%= f.input :price %>
    <%= f.input :address %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :status %>
    <br>
    <%= f.submit "Gravar", class: "btn btn-primary" %>
  <% end %>


